{
	"manifest_version": 2,
	"name": "Barbie VS Oppenheimer Theme Extension",
	"version": "1.0.2",
	"description": "Currently only works on Google.com.",
	"content_scripts": [
		{
			"matches": [
				"https://www.google.com/",
				"http://www.google.com/"
			],
			"js": ["google.js"]
		}
	],
	"permissions": ["tabs"],
	"browser_action": {
		"default_popup": "popup.html"
	}
}
